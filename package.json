{
  "name": "n8n-workflow-sdk",
  "version": "2.0.0",
  "type": "module",
  "description": "Compiler layer for building, deploying, and testing n8n workflows programmatically",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-workflow": "tsx cli/build.ts",
    "validate": "tsx cli/validate.ts",
    "deploy": "tsx cli/deploy.ts",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "dotenv": "^16.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
